<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_version"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.hsdi.NetMe.ui.startup.VersionActivity">

    <android.support.v7.widget.Toolbar
        android:id="@+id/version_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="@color/primary"/>

    <ImageView
        android:id="@+id/version_image"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@mipmap/ic_launcher2"
        android:layout_centerInParent="true"/>

    <TextView
        android:id="@+id/version_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/version_image"
        android:layout_centerHorizontal="true"
        android:layout_margin="20dp"
        android:text="版本号:"/>



</RelativeLayout>
