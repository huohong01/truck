package com.hsdi.NetMe.ui.chat.video;

/** Interface used by the VideoFragment's child fragment to communicate with the VideoFragment. */
public interface VideoInterface {

    /** Alerts the VideoFragment that the video call is being answered */
    void answerCall();

    /** Alerts the VideoFragment that the video call is being declined */
    void denyCall();

    /** Alerts the VideoFragment that the video call was not responded to and is being called */
    void cancelCall();

    void sendNotification();
}
