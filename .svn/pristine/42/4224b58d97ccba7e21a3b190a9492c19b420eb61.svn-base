    <!-- Tutorials -->
    <string name="initial_page_1_title">Welcome</string>
    <string name="initial_page_1_description">NetMe delivers a simple and easy way to instant messaging, audio, or video chat in a secure environment.</string>
    <string name="initial_page_2_title">Phone Number</string>
    <string name="initial_page_2_description">NetMe uses your phone number as your account username. We will need to verify the phone number you enter is yours. This also helps to keep your conversations and data more secure.</string>
    <string name="initial_page_3_title">Lets get started</string>
    <string name="initial_page_3_description">First lets verify your phone number and get you registered and logged in. Then we can continue to show you what NetMe has to offer.</string>
    <string name="main_messages_tab_title">Messages Tab</string>
    <string name="main_messages_tab_description">This tab Lists all your text chats</string>
    <string name="main_messages_toolbar_title">Message Options</string>
    <string name="main_messages_toolbar_description">Options allows you to search text Chats and start a new text chat</string>
    <string name="main_recent_tab_title">Recent Tab</string>
    <string name="main_recent_tab_description">This tab Lists all your video meeting history</string>
    <string name="main_recent_toolbar_title">Recent Options</string>
    <string name="main_recent_toolbar_description">Options allows you to filter meetings by missed, sent, or received calls, and search through meetings</string>
    <string name="main_contact_tab_title">Contacts Tab</string>
    <string name="main_contact_tab_description">This tab Lists all your contacts and shows which ones are registered</string>
    <string name="main_contact_toolbar_title">Contact Options</string>
    <string name="main_contact_toolbar_description">Options allows you to filter registered contacts, search Contacts, refresh the contact list, add a new contact, and share NetMe to multiple people</string>
    <string name="main_favorite_tab_title">Favorites Tab</string>
    <string name="main_favorite_tab_description">This tab Lists all your text chats</string>
    <string name="main_favorite_toolbar_title">Favorite Options</string>
    <string name="main_favorite_toolbar_description">Options allows you to search favorite contacts and add registered contacts as favorites</string>
    <string name="chat_toolbar_title">Chat Options</string>
    <string name="chat_toolbar_description">When in a chat with multiple people you will be allows to change the name of the chat, but when you not in a group chat you have the option to start a video chat with the person your chatting with</string>
    <string name="chat_attachment_title">Attachments</string>
    <string name="chat_attachment_description">You can send voice messages, images, files, or a location through here</string>
